{
  "name": "benefiaciary-registration-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "set PORT=3007 && react-app-rewired start",
    "start:docker": "docker compose up --build -d",
    "stop:docker": "docker compose down",
    "build": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "test:react": "react-app-rewired test",
    "test:func:headless": "npm run start:docker && npx testcafe chrome:headless && npm run stop:docker",
    "test:func": "npm run start:docker && npx testcafe chrome && npm run stop:docker",
    "clean": "npm run clean:npm; npm run clean:build",
    "clean:build": "rm -rf build",
    "clean:npm": "rm -rf node_modules",
    "test": "npm run test:func:headless",
    "lint": "npx eslint src",
    "lint:fix": "npx eslint src --fix",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -u",
    "audit:check": "npx audit-ci --config ./audit-ci.jsonc",
    "audit:fix": "npm audit fix",
    "release": "npx standard-version --no-verify --releaseCommitMessageFormat 'ci(release): {{currentTag}} [skip ci]'",
    "snapshot": "npx standard-version --no-verify --skip.changelog --prerelease snapshot --releaseCommitMessageFormat 'ci(snapshot): {{currentTag}}'",
    "prepare": "husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "web-vitals": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "audit-ci": "^7.0.1",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "npm-check-updates": "^16.14.20",
    "prettier": "^3.3.2",
    "react-app-rewired": "^2.2.1",
    "testcafe": "^3.6.1",
    "typescript-eslint": "^7.13.1"
  },
  "lint-staged": {
    "./src/*.ts": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  }
}
